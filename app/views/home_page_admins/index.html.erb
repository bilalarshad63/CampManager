<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
<div id="main"> 
  <div class="container" style="padding: 20px;"> 
    <div class="main-content card card-block">
      <div class="text-center" style="padding-top: 20px;">
        <h4>Users for Test Resident Camp</h4>

        <div class="row">
        <div class="col">
        <div class="col">
           <h6> <%= "Total Records " + @users.count.to_s + " of " + @users.count.to_s %> </h6>
        </div>
        <div class="col">
          <%= form_with method: :get, local: true do |f| %>
            <%= f.text_field :search,value: params[:search] %>
            <%= f.submit 'Search' %>
          <% end %> 
        </div>
        <%= render(partial: 'home_page_admins/list', locals: { user: @users})%>
        </div>
      </div>
    </div>
  </div>
</div>










