<h3 class="text-center"> Technology Requirements </h3>
<br>
<% if @camp_application.tech_requirements_check? %>
<h6 class="text-center text-success"> Form Submitted </h6>
<%else%>
<h6 class="text-center text-danger"> Form Not Submitted </h6>
<%end%>
<br>
<p>
	<%= f.label :technology_requirements %><br />
	<%= f.text_field :technology_requirements , oninput: 'auto_update_camp_application();',id:'technology-requirements', class: 'w_100'%>
</p>
<br>
<% if @camp_application.calculate_percentage==80 && !@camp_application.tech_requirements_check?  %>
<p class="d-flex justify-content-end"><%= f.submit "Complete", name: "complete_button", class: 'btn btn-success',data: { confirm: 'Are You Sure! After Submission, You cant change the provided info' } %></p>
<%else%>
<p class="d-flex justify-content-end"><%= f.submit "Complete", name: "complete_button", class: 'btn btn-success' %></p>
<%end%>